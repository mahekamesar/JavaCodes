package Strings;

public class SumofStrings {
    public static void main(String[] args) {
        //This programs converts a string to no. & adds with another string
        String s1="111";
        String s2 ="222";
        int sum = Integer.parseInt(s1)+Integer.parseInt(s2);
        System.out.println(sum);
        System.out.println(String.valueOf(sum)); //String

    }
}
